            "settings": {
                "foreground": "#909090"
            }
        },
        {
            "name": "Storage type",
            "scope": "storage.type",
            "settings": {
                "foreground": "#FF7DE9"
            }
        },
        {
            "name": "Other",
            "scope": "variable.language.this",
            "settings": {
                "foreground": "#A7A6C5"
            }
        },
        {
            "name": "Other",
            "scope": [
                "constant.language.boolean.true",
                "constant.language.boolean.false"
