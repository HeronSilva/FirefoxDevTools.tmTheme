{
    "name": "Firefox Dark",
    "tokenColors": [
        {
            "settings": {
                "background": "#2A2A2E",
                "foreground": "#939393"
            }
        },
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#939393"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "foreground": "#E9F4FE"
            }
        },
        {
            "name": "Regexp",
            "scope": "string.regexp",
            "settings": {
                "foreground": "#6B89FF"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#6B89FF"
            }
        },
        {
            "name": "Variable",
            "scope": "variable.language",
            "settings": {
                "foreground": "#75BFFF"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#FF7DE9"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "foreground": "#FFFFB4"
            }
        },
        {
            "name": "Class name",
            "scope": [
                "entity.name.class",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#B98EFF"
            }
        },
        {
            "name": "Function name",
            "scope": "meta.function-call",
            "settings": {
                "foreground": "#B98EFF"
            }
        },
        {
            "name": "Function definition",
            "scope": "meta.definition.function",
            "settings": {
                "foreground": "#75BFFF"
            }
        },
        {
            "name": "Embedded code markers",
            "scope": [
                "punctuation.section.embedded.begin",
                "punctuation.section.embedded.end"
            ],
            "settings": {
                "foreground": "#E9F4FE"
            }
        },
        {
            "name": "Built-in constant",
            "scope": [
                "constant.language",
                "meta.preprocessor"
            ],
            "settings": {
                "foreground": "#E9F4FE"
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other"
            ],
            "settings": {
                "foreground": "#FFFFB4"
            }
        },
        {
            "name": "Inherited class",
            "scope": "entity.other.inherited-class",
            "fontStyle": "italic",
            "settings": {
                "fontStyle": "underline",
                "foreground": "#CC3D3D"
            }
        },
        {
            "name": "Function argument",
            "scope": "variable.parameter",
            "settings": {
                "foreground": "#75BFFF"
            }
        },
        {
            "name": "Punctuation acessor",
            "scope": "punctuation.accessor",
            "settings": {
                "foreground": "#939393"
            }
        },
        {
            "scope": "keyword.type",
            "settings": {
                "foreground": "#6B89FF"
            }
        },
        /* C# */
        {
            "scope": [
                "entity.name.variable.property.cs",
                "entity.name.function.cs"
            ],
            "settings": {
                "foreground": "#E9F4FE"
            }
        },
        {
            "scope": "storage.modifier.cs",
            "settings": {
                "foreground": "#86DE74"
            }
        },
        {
            "scope": "string.quoted.double.cs",
            "settings": {
                "foreground": "#6B89FF"
            }
        },
        {
            "scope": "keyword.other.new.cs",
            "settings": {
                "foreground": "#B98EFF"
            }
        },
        {
            "scope": "entity.name.variable.local.cs",
            "settings": {
                "foreground": "#D8D8E0"
            }
        },
        {
            "scope": "meta.tag.sgml.doctype.html",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#909090"
            }
        },
        {
            "name": "Tag name",
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#75BFFF"
            }
        },
        {
            "name": "Tag start/end",
            "scope": "punctuation.definition.tag",
            "settings": {
                "foreground": "#767578"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "meta.tag.structure.any.html"
            ],
            "settings": {
                "foreground": "#B98EFF"
            }
        },
        {
            "name": "Tag attribute",
            "scope": [
                "entity.other.attribute.name",
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.id.html"
