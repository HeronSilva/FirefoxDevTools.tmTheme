{
  "name": "Firefox DevTools Dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#252C33",
        "foreground": "#B6BABF"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#5C6773"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#EB5368"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.language",
        "variable.other"
      ],
      "settings": {
        "foreground": "#F5F7FA"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#10C970"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword.operator.assignment",
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#A7A6C5"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#10C970"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#10C970"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "meta.preprocessor"
      ],
      "settings": {
        "foreground": "#EB5368"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#EB5368"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "fontStyle": "italic",
      "settings": {
        "foreground": "#EB5368"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#A9BACB"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#10C970"
      }
    },
    {
      "name": "Tag start/end",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#667380"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "meta.tag.structure.any.html",
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html"
      ],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Tag attribute",
      "scope": [
        "entity.other.attribute.name",
        "entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#A3A1C0"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#10C970"
      }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "foreground": "#EB5368"
      }
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#A3A1C0"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "foreground": "#A9BACB"
      }
    },
    {
      "scope": [
        "string.quoted.single",
        "string.quoted.double"
      ],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#A7A6C5"
      }
    },
    // JS
    {
      "name": "Other",
      "scope": [
        "support.function.dom.js",
        "support.variable.property.dom.js",
        "variable.other.property.js",
        "constant.language.undefined.js",
        "variable.other.object.property.js",
        "keyword.operator.comparison.js"
      ],
      "settings": {
        "foreground": "#A7A6C5"
      }
    },
    {
      "name": "Other",
      "scope": "variable.language.this.js",
      "settings": {
        "foreground": "#10C970"
      }
    },
    {
      "name": "Other",
      "scope": [
        "support.variable.dom.js",
        "support.function.js",
        "variable.other.object.js",
        "variable.other.readwrite.js"
      ],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function.js",
      "settings": {
        "foreground": "#A7A6C5"
      }
    },
    {
      "name": "Other",
      "scope": "meta.brace.square.js",
      "settings": {
        "foreground": "#A7A6C5"
      }
    },
    {
      "name": "Other",
      "scope": [
        "storage.type.function.js",
        "storage.type.js"
      ],
      "settings": {
        "foreground": "#10C970"
      }
    },
    // CSS
    {
      "name": "Other",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#10C970"
      }
    },
    {
      "name": "Other",
      "scope": [
        "constant.numeric.css",
        "support.function.url.css",
        "meta.function.url.css",
        "support.function.misc.css",
        "keyword.other.unit.px.css",
        "keyword.other.unit.pt.css",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.em.css",
        "keyword.other.unit.rem.css",
        "keyword.other.unit.ch.css",
        "keyword.other.unit.vh.css",
        "keyword.other.unit.vw.css",
        "keyword.other.unit.s.css",
        "meta.property-value.css",
        "support.constant.property-value.css",
        "keyword.other.unit.scss"
      ],
      "settings": {
        "foreground": "#EB5368"
      }
    },
    {
      "name": "Other",
      "scope": [
        "constant.other.color.rgb-value.hex.css",
        "variable.parameter.url.css",
        "support.function.transform.css",
        "support.function.calc.css",
        "entity.other.attribute-name.pseudo-class.css",
        "meta.function.gradient.css",
        "support.function.gradient.css",
        "entity.name.tag.css",
        "variable.parameter.url.scss",
        "entity.name.tag.scss"
      ],
      "settings": {
        "foreground": "#E9F4FE"
      }
    },
    {
      "name": "Other",
      "scope": [
        "punctuation.terminator.rule.css",
        "punctuation.separator.key-value.css",
        "punctuation.section.property-list.begin.bracket.curly.css",
        "punctuation.section.property-list.end.bracket.curly.css",
        "punctuation.section.function.begin.bracket.round.css",
        "punctuation.section.function.end.bracket.round.css"
      ],
      "settings": {
        "foreground": "#A9BACB"
      }
    },
    {
      "name": "Other",
      "scope": "support.type.vendored.property-name.css",
      "settings": {
        "foreground": "#667382"
      }
    },
    // PHP
    {
      "name": "Other",
      "scope": "variable.other.property.php",
      "settings": {
        "foreground": "#B6BABF"
      }
    }
  ],
  "colors": {
    "editorBackground": "#252C33",
    "editorCursor": "#B6BABF",
    "editorWhitespaces": "#5C6773",
    "editorLineHighlight": "#5E88B033",
    "editorSelection": "#1D4F73aa",
    "sideBarBackground": "#2B303A",
    "statusBarBackground": "#393F4C",
    "activityBarBackground": "#393F4C",
    "activityBadgeBackground": "#EB5368",
    "sideBarTitleForeground": "#B6BABF",
    "inputBoxBackground": "#393F4C",
    "inputBoxForeground": "#B6BABF",
    "inputBoxActiveOptionBorder": "#EB5368",
    "activeTabBackground": "#393F4C",
    "inactiveTabBackground": "#272B35",
    "tabsContainerBackground": "#272B35",
    "focusedElementOutline": "#B6BABF"
  }
}
