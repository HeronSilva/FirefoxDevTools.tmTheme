{
    "name": "firefox-devtools-theme",
    "displayName": "Firefox DevTools Theme",
    "description": "Mozilla Firefox developer tools theme for VS Code.",
    "version": "1.0.0",
    "publisher": "Heron",
    "repository": {
        "url": "https://github.com/HeronSilva/FirefoxDevTools.tmTheme/"
    },
    "icon": "icon.png",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Firefox DevTools Light Theme",
                "uiTheme": "vs",
                "path": "./themes/FirefoxDevToolsLight.json"
            },
            {
                "label": "Firefox DevTools Dark Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/FirefoxDevToolsDark.json"
            },
            {
                "label": "Firefox DevTools Darker Theme",
                "uiTheme": "vs-dark",
                "path": "./themes/FirefoxDevToolsDarker.json"
            }
        ]
    }
}
